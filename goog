#!/usr/bin/python

import sys
import string
import codecs
import os
import google

google.LICENSE_KEY = 'gHbcd0RQFHKbR2PQ7o4q/LKfCMVB03Ru'
sys.stdout = codecs.lookup('utf-8')[-1](sys.stdout)

argk = 1
argc = len(sys.argv)
query = ' '
while argk < argc:
    query = query + sys.argv[argk] + ' '
    argk = argk+1
data = google.doGoogleSearch(query)

# In the case of the command line, usually we want to see the last results
# first as the others scroll off the screen

data.results.reverse()
for result in data.results:
    print result.URL 
